{
	"name": "Pure Qrcode",
	"manifest_version": 2,
	"version": "1.0",
	"description": "Pure qrcode extension for chrome",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "app.html"
	},
	"permissions": [
		"tabs",
		"<all_urls>"
	]
}